<!DOCTYPE api-data SYSTEM '../../dtd/api-data.dtd'>
<api-data>
    <api-path>/user/login</api-path>
    <http-request />
    <main-class>
        <class class-name="LoginResponse">
            <description>登录响应</description>
            <int>
                <key>code</key>
                <description>响应代码: 0 - login successfully, 1 - user not found, 2 - wrong password, 3 - login already, 404 - error</description>
            </int>
            <string>
                <key>session</key>
                <description>会话ID</description>
            </string>
        </class>
    </main-class>
</api-data>